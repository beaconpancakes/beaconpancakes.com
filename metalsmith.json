{
  "metadata": {
    "url": "http://beaconpancakes.com/",
    "sitename": "Beatriz Alonso - Online Portfolio",
    "twitter": {
      "user": "@beaconpancakes"
    }
  },
  "source": "src",
  "destination": "docs",
  "plugins": {
    "metalsmith-collections": {
      "projects": {
        "pattern": "projects/*.md",
        "sortBy": "order"
      }
    },
    "metalsmith-markdown": true,
    "metalsmith-permalinks": {
      "relative": false,
      "pattern": "projects/:title"
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    }
  }
}
