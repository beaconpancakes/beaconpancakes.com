{
  "source": "src",
  "destination": "docs",
  "plugins": {
    "metalsmith-markdown": true,
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-collections": {
      "projects": "src/projects/*.md"
    },
    "metalsmith-permalinks": "projects/:title"
  }
}
