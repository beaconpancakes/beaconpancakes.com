{
  "metadata": {
    "sitename": "Beatriz Alonso - Online Portfolio"
  },
  "source": "src",
  "destination": "docs",
  "plugins": {
    "metalsmith-collections": {
      "projects": "projects/*.md"
    },
    "metalsmith-markdown": true,
    "metalsmith-permalinks": {
      "relative": false,
      "pattern": "projects/:title"
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    }
  }
}
